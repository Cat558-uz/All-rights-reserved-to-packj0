
-- script create BY ghost

local TweenService = game:GetService("TweenService")
local Player = game.Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

-- ScreenGui principal
local NotifyGui = PlayerGui:FindFirstChild("NotifyGui") or Instance.new("ScreenGui")
NotifyGui.Name = "NotifyGui"
NotifyGui.Parent = PlayerGui
NotifyGui.ResetOnSpawn = false

-- Pasta para notificações
local NotifFolder = NotifyGui:FindFirstChild("NotifFolder") or Instance.new("Folder")
NotifFolder.Name = "NotifFolder"
NotifFolder.Parent = NotifyGui

-- Configurações
local NOTIF_WIDTH = 260
local NOTIF_HEIGHT = 60
local NOTIF_SPACING = 8
local MAX_NOTIFS = 3
local BASE_POSITION = UDim2.new(1, -280, 1, -80)

function Notify(imageId, text, time)
	local ok, err = pcall(function()
		time = time or 3
		imageId = imageId or 6031075932
		text = text or "Notificação"

		for _, oldNotif in ipairs(NotifFolder:GetChildren()) do
			if oldNotif:IsA("Frame") then
				local newY = oldNotif.Position.Y.Offset - (NOTIF_HEIGHT + NOTIF_SPACING)
				TweenService:Create(oldNotif, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
					Position = UDim2.new(1, -280, 1, newY)
				}):Play()
			end
		end

		if #NotifFolder:GetChildren() >= MAX_NOTIFS then
			local oldest = NotifFolder:GetChildren()[1]
			if oldest then oldest:Destroy() end
		end

		local Frame = Instance.new("Frame")
		Frame.Size = UDim2.new(0, NOTIF_WIDTH, 0, NOTIF_HEIGHT)
		Frame.Position = UDim2.new(1, 300, 1, -80)
		Frame.AnchorPoint = Vector2.new(0, 1)
		Frame.BackgroundTransparency = 0
		Frame.BorderSizePixel = 0
		Frame.Parent = NotifFolder
		Frame.ZIndex = 10

		local UIGradient = Instance.new("UIGradient", Frame)
		UIGradient.Color = ColorSequence.new({
			ColorSequenceKeypoint.new(0, Color3.fromRGB(15, 15, 15)),
			ColorSequenceKeypoint.new(1, Color3.fromRGB(120, 0, 0))
		})

		local UICorner = Instance.new("UICorner", Frame)
		UICorner.CornerRadius = UDim.new(0, 10)

		local UIStroke = Instance.new("UIStroke", Frame)
		UIStroke.Thickness = 1.6
		UIStroke.Color = Color3.fromRGB(255, 0, 70)
		UIStroke.Transparency = 0.1

		local Image = Instance.new("ImageLabel", Frame)
		Image.Size = UDim2.new(0, 45, 0, 45)
		Image.Position = UDim2.new(0, 8, 0.5, -22)
		Image.BackgroundTransparency = 1
		Image.Image = "rbxassetid://" .. tostring(imageId)
		Image.ZIndex = 11

		local Label = Instance.new("TextLabel", Frame)
		Label.Size = UDim2.new(1, -65, 1, -10)
		Label.Position = UDim2.new(0, 60, 0, 5)
		Label.BackgroundTransparency = 1
		Label.Text = text
		Label.TextColor3 = Color3.fromRGB(255, 255, 255)
		Label.TextSize = 16
		Label.Font = Enum.Font.GothamBold
		Label.TextWrapped = true
		Label.TextXAlignment = Enum.TextXAlignment.Left
		Label.ZIndex = 11

		local RedzMark = Instance.new("TextLabel", Frame)
		RedzMark.Size = UDim2.new(0, 80, 0, 14)
		RedzMark.Position = UDim2.new(1, -85, 1, -15)
		RedzMark.BackgroundTransparency = 1
		RedzMark.Text = "Redz Hub"
		RedzMark.TextColor3 = Color3.fromRGB(255, 0, 70)
		RedzMark.TextSize = 12
		RedzMark.Font = Enum.Font.GothamSemibold
		RedzMark.TextXAlignment = Enum.TextXAlignment.Right
		RedzMark.ZIndex = 12

		TweenService:Create(Frame, TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {
			Position = BASE_POSITION
		}):Play()

		local Sound = Instance.new("Sound", Frame)
		Sound.SoundId = "rbxassetid://8486683243"
		Sound.Volume = 0.8
		Sound:Play()

		task.delay(time, function()
			TweenService:Create(Frame, TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.In), {
				Position = UDim2.new(1, 300, 1, Frame.Position.Y.Offset)
			}):Play()
			task.wait(0.4)
			Frame:Destroy()
		end)
	end)

	if not ok then
		warn("Erro na notificação:", err)
	end
end

