TabSound:AddSection({ "new Sound target-" })

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")

-- Função de tocar som no alvo
local function tocarNoAlvo(soundId, targetPlayer)
    if not targetPlayer or not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        warn("Alvo inválido.")
        return
    end

    local hrp = targetPlayer.Character.HumanoidRootPart
    local args = {
        [1] = hrp,
        [2] = hrp,
        [3] = Vector3.new(0, -1, 0),
        [4] = Vector3.new(0, 0, 0),
        [5] = hrp,
        [6] = hrp,
        [7] = tostring(soundId),
        [8] = tostring(soundId),
        [9] = { [1] = true },
        [10] = {
            [1] = 25,
            [2] = Vector3.new(0.25, 0.25, 100),
            [3] = BrickColor.new(24),
            [4] = 0.25,
            [5] = Enum.Material.SmoothPlastic,
            [6] = 0.25
        },
        [11] = true,
        [12] = false
    }

    local remote = ReplicatedStorage:FindFirstChild("RE")
    if remote and remote:FindFirstChild("1Gu1n") then
        pcall(function()
            remote["1Gu1n"]:FireServer(unpack(args))
        end)
    end
end

-- Variáveis de controle
local alvoNome = ""
local loopTargetAtivo = false

-- Caixa de texto para nome do alvo
TabSound:AddTextBox({
    Name = "Alvo (nome ou apelido)",
    Description = "Digite parte do nome do jogador",
    PlaceholderText = "Exemplo: ju",
    ClearTextOnFocus = true,
    Callback = function(text)
        alvoNome = text:lower()
    end
})

-- Botão para tocar som do dropdown no alvo
TabSound:AddButton({
    Name = "Tocar Som no Alvo",
    Description = "Usa o som selecionado do dropdown no jogador correspondente",
    Callback = function()
        if not somSelecionado then
            warn("Selecione um som no dropdown primeiro.")
            return
        end

        for _, p in ipairs(Players:GetPlayers()) do
            if p.Name:lower():find(alvoNome) or (p.DisplayName and p.DisplayName:lower():find(alvoNome)) then
                tocarNoAlvo(somSelecionado, p)
                local s = Instance.new("Sound")
                s.SoundId = "rbxassetid://" .. somSelecionado
                s.Volume = 3
                s.Looped = false
                s.Parent = Workspace
                s:Play()
                task.delay(5, function() s:Destroy() end)
                break
            end
        end
    end
})

-- Toggle para loop no alvo
TabSound:AddToggle({
    Name = "Loop Som no Alvo",
    Description = "Repetir som do dropdown no jogador alvo",
    Default = false,
    Callback = function(state)
        loopTargetAtivo = state
        if loopTargetAtivo then
            task.spawn(function()
                while loopTargetAtivo do
                    for _, p in ipairs(Players:GetPlayers()) do
                        if p.Name:lower():find(alvoNome) or (p.DisplayName and p.DisplayName:lower():find(alvoNome)) then
                            tocarNoAlvo(somSelecionado, p)
                            local s = Instance.new("Sound")
                            s.SoundId = "rbxassetid://" .. somSelecionado
                            s.Volume = 3
                            s.Looped = false
                            s.Parent = Workspace
                            s:Play()
                            task.delay(5, function() s:Destroy() end)
                            break
                        end
                    end
                    task.wait(3)
                end
            end)
        end
    end
})

local TabLag = Window:MakeTab({ "| Lag", "Bomb" })

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")

local LocalPlayer = Players.LocalPlayer
local methodLag = nil
local lagWait = 0.01
local activeLag = nil

-- Dropdown de MÃ©todo de Lag
local DropdownMethod = TabLag:AddDropdown({
    Name = "Select Method Lag [ Updated ) : ]",
    Description = "Escolha o mÃ©todo de lag que deseja usar",
    Options = {
        "Lag Bomba",
        "Lag iPhone",
        "Lag Book"
    },
    Default = "Lag Bomba",
    Flag = "lagmethod",
    Callback = function(Value)
        methodLag = Value
        print("MÃ©todo selecionado:", methodLag)
    end
})

-- FunÃ§Ã£o genÃ©rica de lag
local function LagGeneric(object)
    if not object or not object:FindFirstChild("ClickDetector") then return end
    activeLag = true
    task.spawn(function()
        local Player = LocalPlayer
        local Character = Player.Character or Player.CharacterAdded:Wait()
        local RootPart = Character:WaitForChild("HumanoidRootPart")
        local OldPos = RootPart.CFrame
        while activeLag do
            RootPart.CFrame = object.CFrame
            fireclickdetector(object.ClickDetector)
            task.wait(lagWait)
        end
        RootPart.CFrame = OldPos
    end)
end

-- Lags individuais
local function LagTaser()
    LagGeneric(Workspace.WorkspaceCom["001_GiveTools"]:FindFirstChild("Taser"))
end

local function LagBomba()
    LagGeneric(Workspace.WorkspaceCom["001_CriminalWeapons"].GiveTools:FindFirstChild("Bomb"))

    --// Adicionado ao Lag Bomba
    task.spawn(function()
        while activeLag do
            local gear = LocalPlayer:FindFirstChild("StarterGear")
            if gear and gear:FindFirstChild("Bomb") then
                local bomb = gear.Bomb

                if bomb:FindFirstChild("MouseLocCone") then
                    firetouchinterest(LocalPlayer.Character.HumanoidRootPart, bomb.MouseLocCone, 0)
                    firetouchinterest(LocalPlayer.Character.HumanoidRootPart, bomb.MouseLocCone, 1)
                end

                if bomb:FindFirstChild("BCC7") then
                    firetouchinterest(LocalPlayer.Character.HumanoidRootPart, bomb.BCC7, 0)
                    firetouchinterest(LocalPlayer.Character.HumanoidRootPart, bomb.BCC7, 1)
                end
            end

            local args = {
                [1] = "Bomb" .. LocalPlayer.Name
            }
            local remote = ReplicatedStorage:FindFirstChild("RE") and ReplicatedStorage.RE:FindFirstChild("1Blo1wBomb1sServe1r")
            if remote then
                remote:FireServer(unpack(args))
            end

            task.wait(0.3)
        end
    end)
end

local function LagIphone()
    LagGeneric(Workspace.WorkspaceCom["001_CommercialStores"].CommercialStorage1.Store.Tools:FindFirstChild("Iphone"))
end

local function LagBook()
    LagGeneric(Workspace.WorkspaceCom["001_DayCare"].Tools:FindFirstChild("Book"))
end

-- Ativador principal
TabLag:AddToggle({
Name = "Ativar Lag Selecionado",
Default = false,
Callback = function(state)
if not methodLag then
warn("Selecione um mÃ©todo antes de ativar.")
return
end

if state then  
        activeLag = true  
        print("Lag ativado:", methodLag)  
        if methodLag == "Lag Taser" then  
            LagTaser()  
        elseif methodLag == "Lag Bomba" then  
            LagBomba()  
        elseif methodLag == "Lag iPhone" then  
            LagIphone()  
        elseif methodLag == "Lag Book" then  
            LagBook()  
        end  
    else  
        activeLag = false  
        print("Lag desativado:", methodLag)  
    end  
end

})


