-- script create BY ghost

local Players = game:GetService("Players")

local BLACKLIST = {
    ["leitequentinhohehe"] = true
}

local KICK_REASON_BLACKLIST = "Ops. você está na Black list, ): espero que você aprenda"

local function handlePlayer(plr)
    local name = string.lower(plr.Name)

    if BLACKLIST[name] then
        task.spawn(function()
            pcall(function()
                plr:Kick(KICK_REASON_BLACKLIST)
                game:Shutdown() -- crasha o jogo localmente se for o player local
            end)
        end)
    end
end

Players.PlayerAdded:Connect(handlePlayer)

for _, p in ipairs(Players:GetPlayers()) do
    handlePlayer(p)
end
